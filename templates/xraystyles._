Map { background-color:#000; }

<%
var disabled = obj._prefs.disabled || [];
_(obj.vector_layers).each(function(l, i) {
var id = l.id;
if (disabled.indexOf(id) !== -1) return;
%>
#<%=id%>['mapnik::geometry_type'=1] {
  marker-allow-overlap:true;
  marker-width:4;
  marker-height:4;
  marker-line-width:0;
  marker-fill:<%=this.xraycolor(id)%>;
  text-allow-overlap:true;
  text-align:left;
  text-name:'[name]';
  text-face-name:"Source Sans Pro Bold";
  text-fill:<%=this.xraycolor(id)%>;
  text-size:12;
  text-dy:6;
  comp-op:screen;
}
#<%=id%>['mapnik::geometry_type'=2] {
  line-width:1;
  line-color:<%=this.xraycolor(id)%>;
  comp-op:screen;
}
#<%=id%>['mapnik::geometry_type'=3] {
  line-width:0.5;
  line-color:<%=this.xraycolor(id)%>;
  polygon-fill:fadeout(<%=this.xraycolor(id)%>,40);
  comp-op:screen;
}
<% }.bind(this)); %>
